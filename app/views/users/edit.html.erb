<h1>Edit
  <%= @user.user_name %>
</h1>

<%= form_for @user do |f| %>
  <%= f.label :user_name %>
  <%= f.text_field :user_name %>
  <%= f.label :first_name %>
  <%= f.text_field :first_name %>
  <%= f.label :last_name %>
  <%= f.text_field :last_name %>
  <%= f.label :street_address %>
  <%= f.text_field :street_address %>
  <%= f.label :city %>
  <%= f.text_field :city %>
  <%= f.label :state %>
  <%= f.text_field :state %>
  <%= f.label :zip %>
  <%= f.number_field :zip %>
  <%= f.label :password %>
  <%= f.password_field :password %>
  <%= f.submit 'Update Info'%>
<% end %>